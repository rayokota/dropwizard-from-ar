<#-- @ftlvariable name="" type="com.yammer.<%= service_name %>.service.views.<%= model.cap_first %>ShowView" -->
<#setting number_format="computer">
<#include "../header.ftl"/>
<div class="page-header">
<h1>Showing <%= model.cap_first %> ${<%= model %>.id}</h1>
</div>

<#if notice != "">
<div class="alert alert-success">
<p id="notice">${notice}</p>
</div>
</#if>

<dl class="dl-horizontal">
  <dt>ID:</dt>
  <dd>${<%= model %>.id}</dd>
<% attrs.each_pair do |k, v| %>
  <dt><%= k.cap_first %>:</dt>
  <dd>${(<%= model %>.<%= k %>!"&nbsp;")<%= if v == "boolean" then "?string(\"true\", \"false\")" else "" end %>}</dd>
<% end %>
</dl>

<a href="/<%= model_plural %>" class="btn">Back</a>
<#include "../footer.ftl"/>
